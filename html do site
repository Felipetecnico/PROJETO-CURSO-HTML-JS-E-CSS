               <!-- Campo de busca -->
<div id="divBusca" style="margin: 20px; text-align: center;">
  <img src="search3.png" alt="Buscar..." style="width:20px; vertical-align:middle;"/>
  <input type="text" id="txtBusca" placeholder="Buscar por cidade..." />
  <button id="btnBusca">Buscar</button>
</div>

<!-- Script de busca -->
<script>
  document.getElementById("btnBusca").addEventListener("click", function () {
    let termo = document.getElementById("txtBusca").value.toLowerCase();
    let imoveis = document.querySelectorAll(".portfolio-item");

    imoveis.forEach(function (item) {
      let cidade = item.querySelector("h4").innerText.toLowerCase();
      if (cidade.includes(termo) || termo === "") {
        item.parentElement.style.display = "block"; // mostra
      } else {
        item.parentElement.style.display = "none"; // esconde
      }
    });
  });

  // Ativa busca ao digitar (opcional)
  document.getElementById("txtBusca").addEventListener("keyup", function (e) {
    if (e.key === "Enter") {
      document.getElementById("btnBusca").click();
    }
  });
</script>


